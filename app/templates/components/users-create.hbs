<form class="login-screen">
  <h2>Username</h2>
  {{input type="text" name="emailAddress" value=model.emailAddress placeholder="Email address"}}
  <h2>Password</h2>
  {{input type="password" name="password" value=model.password placeholder="Password"}}
  <button class="submit" type="button" name="login-button" {{action 'createUser'}}>Sign Up</button>
  <div class="button-container">
  </div>
</form>
{{yield}}
